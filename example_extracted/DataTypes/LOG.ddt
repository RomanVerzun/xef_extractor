(* Log structure *)
(* ======================================== *)

TYPE LOG :
STRUCT
  Messages : ARRAY[0..100] OF LOG_Buff; (* Array of messages *)
  LogIndex : INT; (* Index of current log *)
  LogCount : INT; (* Log count *)
END_STRUCT;
END_TYPE