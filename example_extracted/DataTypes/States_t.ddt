(* DATA TYPE: States_t *)
(* VERSION: 0.05 *)
(* Valve states based on Limit switches *)
(* ======================================== *)

TYPE States_t :
STRUCT
  Open : INT; (* Open *)
  Close : INT; (* Close *)
  Error : INT; (* Both active *)
  Middle : INT; (* No active *)
END_STRUCT;
END_TYPE